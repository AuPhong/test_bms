<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  https://axelor.com/xml/ns/object-views/object-views_6.0.xsd">

    <grid title="" name="apartment-grid" model="com.axelor.bms.db.Apartment">
        <field name="block"></field>
        <field name="code"></field>
        <field name="name"></field>
        <field name="buildUpArea"></field>
        <field name="carpetArea"></field>
        <field name="managementFeeType"></field>
        <field name="waterFeeType"></field>
        <field name="handOverDate"></field>
        <field name="chargeDate"></field>
    </grid>

    <form title="" name="apartment-form" model="com.axelor.bms.db.Apartment">
        <panel name="mainPanel">
            <field name="block"></field>
            <field name="code"></field>
            <field name="name"></field>
            <field name="buildUpArea"></field>
            <field name="carpetArea"></field>
            <field name="managementFeeType"></field>
            <field name="waterFeeType"></field>
            <field name="handOverDate"></field>
            <field name="chargeDate"></field>
        </panel>
    </form>

    <action-view name="action.apartment.view.all" title="" model="com.axelor.bms.db.Apartment">
        <view type="grid" name="apartment-grid"></view>
        <view type="form" name="apartment-form"></view>
    </action-view>
</object-views>